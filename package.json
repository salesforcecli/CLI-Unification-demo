{
  "name": "@salesforce/sf-demo",
  "description": "An experiment to explore a new taxonomy for all Salesforce clouds.",
  "version": "1.3.1",
  "author": "Salesforce",
  "keywords": [],
  "license": "BSD-3-Clause",
  "repository": "salesforcecli/cli-taxonomy-experiment",
  "homepage": "https://github.com/salesforcecli/cli-taxonomy-experiment",
  "bugs": "https://github.com/salesforcecli/cli-taxonomy-experiment/issues",
  "bin": {
    "sf": "bin/run"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/bin"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sf",
    "topicSeparator": " ",
    "topics": {
      "env": {
        "description": "Commands to work with enviornments."
      },
      "login": {
        "description": "Commands for specialized login flows."
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^0.5.5",
    "@salesforce/core": "^2.15.4",
    "@salesforce/kit": "^1.5.0",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "inquirer": "^8.0.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^2.1.0",
    "@salesforce/dev-scripts": "0.9.1",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "1.3.5",
    "@types/inquirer": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.2.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "oclif": "^1.17.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "shx": "0.3.3",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sf-husky-pre-commit",
      "commit-msg": "sf-husky-commit-msg",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
